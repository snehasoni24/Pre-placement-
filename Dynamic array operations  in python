arr = []
while True:
    ch = int(input("1.Insert 2.Delete 3.Display 4.Exit: "))
    if ch == 1:
        arr.append(int(input("Value: ")))
    elif ch == 2 and arr:
        pos = int(input("Pos: "))
        if 0 <= pos < len(arr): arr.pop(pos)
    elif ch == 3:
        print(arr)
    else:
        break
